# --- RPC & Keys ---
RPC_URL=https://sepolia.infura.io/v3/<api-key>
PRIVATE_KEY=0xabc123... # without quotes

# --- Core Contracts ---
OM_ASSET=0x0000000000000000000000000000000000000000 # leave zero to deploy mock
OM_ATOKEN=0x... # aToken address (e.g., aUSDC)
OM_AAVE_POOL=0x... # Aave Pool address
OM_ADMIN=0x... # (optional) protocol admin, defaults to signer
OM_INITIAL_MINT=1000000000000000000000000 # optional, mock asset mint to admin

# --- Program Seeding (set OM_PROGRAM_COUNT, then define each program) ---
OM_PROGRAM_COUNT=2
OM_PROGRAM_0_RECIPIENT=0x...
OM_PROGRAM_0_BPS=7000
OM_PROGRAM_0_URI=ipfs://program0
OM_PROGRAM_0_ACTIVE=true

OM_PROGRAM_1_RECIPIENT=0x...
OM_PROGRAM_1_BPS=3000
OM_PROGRAM_1_URI=ipfs://program1
OM_PROGRAM_1_ACTIVE=true
